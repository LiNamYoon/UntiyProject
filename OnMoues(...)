// 마우스 체크하는 유니티 이벤트 함수

using UnityEngine;
using System.Collections;

public class OnMouse : MonoBehaviour 
{

    void OnMouseDown()
    {
        Debug.Log("마우스 누름");
    }

    void OnMouseStay()
    {
        Debug.Log("마우스 누르는 중");
    }

    void OnMouseUP()
    {
        Debug.Log("마우스 땜");
    }

}


